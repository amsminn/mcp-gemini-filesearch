{
  "name": "mcp-gemini-filesearch",
  "version": "1.0.0",
  "description": "MCP server wrapping Google Gemini File Search API for document-based RAG",
  "type": "module",
  "main": "dist/server.js",
  "bin": {
    "mcp-gemini-filesearch": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node bin/cli.js",
    "dev": "tsx src/server.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --maxWorkers=2",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test:ci && npm run build",
    "postpublish": "git push -u origin main --follow-tags || echo 'Warning: Failed to push tags'",
    "prepack": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "file-search",
    "rag",
    "document-search",
    "retrieval-augmented-generation",
    "google-ai",
    "nlp",
    "semantic-search"
  ],
  "author": "MCP-Gemini-FileSearch Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amsminn/mcp-gemini-filesearch.git"
  },
  "bugs": {
    "url": "https://github.com/amsminn/mcp-gemini-filesearch/issues"
  },
  "homepage": "https://github.com/amsminn/mcp-gemini-filesearch#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "manifest.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
